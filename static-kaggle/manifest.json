{
  "name": "Mouse Dictionary Kaggle support",
  "description": "Mouse Dictionary Kaggle support",
  "author": "wtetsu",
  "version": "0.1.3",
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content.js"],
      "matches": ["https://www.kaggleusercontent.com/*"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Mouse Dictionary Kaggle support"
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
