{
  "name": "mouse-dictionary-iframe-support",
  "version": "1.0.0",
  "repository": "https://github.com/wtetsu/mouse-dictionary-iframe.git",
  "author": "wtetsu",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.common.js",
    "watch": "webpack -w --config webpack.config.common.js",
    "release": "NODE_ENV=production npm run build",
    "build-kaggle": "webpack --config webpack.config.kaggle.js",
    "watch-kaggle": "webpack -w --config webpack.config.kaggle.js",
    "release-kaggle": "NODE_ENV=production npm run build-kaggle",
    "build-allurls": "webpack --config webpack.config.allurls.js",
    "watch-allurls": "webpack -w --config webpack.config.allurls.js",
    "release-allurls": "NODE_ENV=production npm run build-allurls",
    "lint": "eslint src/**/*.{js,jsx} && prettier --check src/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "lodash": "^4.17.15",
    "milligram": "^1.3.0",
    "prettier": "^2.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
