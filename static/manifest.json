{
  "name": "Mouse Dictionary iframe",
  "description": "Mouse Dictionary iframe",
  "author": "wtetsu",
  "version": "0.0.1",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": ["activeTab", "https://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "webextensions-webpack-boilerplate"
  },
  "manifest_version": 2,
  "icons": {},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
