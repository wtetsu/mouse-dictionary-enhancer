{
  "name": "Mouse Dictionary iframe support",
  "description": "Mouse Dictionary iframe support",
  "author": "wtetsu",
  "version": "0.0.1",
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content.js"],
      "matches": ["https://www.kaggleusercontent.com/*"]
    }
  ],
  "browser_action": {
    "default_title": "Mouse Dictionary iframe support"
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
