{
  "name": "Mouse Dictionary iframe support",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "author": "wtetsu",
  "version": "1.0.3",
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "match_about_blank": true,
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Mouse Dictionary iframe support"
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
